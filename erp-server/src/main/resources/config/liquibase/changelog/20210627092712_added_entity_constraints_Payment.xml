<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Payment.
    -->
    <changeSet id="20210627092712-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tax_rule_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment__tax_rule_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tax_rule"/>

        <addForeignKeyConstraint baseColumnNames="payment_category_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment__payment_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_category"/>

        <addForeignKeyConstraint baseColumnNames="payment_calculation_id"
                                 baseTableName="payment"
                                 constraintName="fk_payment__payment_calculation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_calculation"/>
    </changeSet>
</databaseChangeLog>
